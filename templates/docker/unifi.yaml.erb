---
version: '3.4'
services:
  unifi:
    image: 'linuxserver/unifi:latest'
    volumes:
      - '/media/tb-tb/unifi:/config'
    restart: 'always'
    ports:
      - '3478:3478/udp'
      - '10001:10001/udp'
      - '8080:8080'
      - '8081:8081'
      - '8443:8443'
      - '8843:8843'
      - '8880:8880'
